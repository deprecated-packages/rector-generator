{
    "name": "rector/rector-generator",
    "type": "rector-extension",
    "description": "Generate Rector rules from command line",
    "homepage": "https://getrector.org",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "rector/rector-src": "^0.11.15",
        "nikic/php-parser": "^4.10.5",
        "symfony/console": "^4.4.8|^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/finder": "^4.4.8|^5.1",
        "symplify/astral": "^9.3",
        "symplify/package-builder": "^9.3",
        "danielstjules/stringy": "^3.1"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpunit/phpunit": "^9.5",
        "symplify/coding-standard": "^9.3",
        "symplify/easy-coding-standard": "^9.3",
        "symplify/phpstan-extensions": "^9.3",
        "symplify/phpstan-rules": "^9.3",
        "symplify/easy-testing": "^9.2",
        "phpstan/extension-installer": "^1.1",
        "rector/rector-phpstan-rules": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Rector\\RectorGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\RectorGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpstan": "vendor/bin/phpstan analyse --ansi --error-format symplify"
    },
    "extra": {
        "rector": {
            "includes": [
                "config/config.php"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

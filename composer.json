{
    "name": "rector/rector-generator",
    "type": "rector-extension",
    "description": "Generate Rector rules from command line",
    "homepage": "https://getrector.org",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "nette/utils": "^3.2",
        "nikic/php-parser": "^4.14",
        "symfony/console": "^6.2",
        "symfony/dependency-injection": "6.1.*",
        "symfony/finder": "^6.2",
        "symplify/package-builder": "^11.1",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "rector/rector": "^0.15.13",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "^10.0",
        "phpstan/phpdoc-parser": "^1.6.4",
        "symplify/coding-standard": "^11.0",
        "symplify/easy-coding-standard": "^11.0",
        "symplify/phpstan-extensions": "^11.0",
        "symplify/phpstan-rules": "^11.0",
        "phpstan/extension-installer": "^1.1",
        "symplify/vendor-patches": "^11.0",
        "phpstan/phpstan-webmozart-assert": "^1.0",
        "tracy/tracy": "^2.9",
        "symplify/easy-ci": "^11.2.0",
        "symplify/easy-testing": "^11.1"
    },
    "autoload": {
        "psr-4": {
            "Rector\\RectorGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\RectorGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpstan": "vendor/bin/phpstan analyse --ansi --error-format symplify"
    },
    "extra": {
        "enable-patching": true,
        "rector": {
            "includes": [
                "config/config.php"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "rector/extension-installer": true,
            "phpstan/extension-installer": true
        }
    }
}

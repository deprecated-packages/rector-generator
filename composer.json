{
    "name": "rector/rector-generator",
    "type": "rector-extension",
    "description": "Generate Rector rules from command line",
    "homepage": "https://getrector.org",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "nette/utils": "^3.2",
        "nikic/php-parser": "^4.16.0",
        "symfony/console": "^6.2",
        "symfony/finder": "^6.2",
        "webmozart/assert": "^1.11",
        "symfony/filesystem": "^6.3"
    },
    "require-dev": {
        "rector/rector": "^0.17",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "^10.3",
        "phpstan/phpdoc-parser": "^1.23",
        "symplify/coding-standard": "^11.0",
        "symplify/easy-coding-standard": "^12.0",
        "symplify/phpstan-extensions": "^11.0",
        "symplify/phpstan-rules": "^12.0",
        "phpstan/extension-installer": "^1.1",
        "symplify/vendor-patches": "^11.0",
        "phpstan/phpstan-webmozart-assert": "^1.2",
        "tracy/tracy": "^2.10",
        "symplify/easy-ci": "^11.3",
        "symplify/easy-testing": "^11.1"
    },
    "autoload": {
        "psr-4": {
            "Rector\\RectorGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\RectorGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpstan": "vendor/bin/phpstan analyse --ansi --error-format symplify"
    },
    "extra": {
        "enable-patching": true,
        "rector": {
            "includes": [
                "config/config.php"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "rector/extension-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
